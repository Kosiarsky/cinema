# Backend environment configuration
# Copy this file to .env and adjust values per environment

# Allowed frontend origin(s) for CORS (comma-separated)
CORS_ALLOW_ORIGINS=http://localhost:4200

# Frontend base URL for building redirect links (e.g., Stripe success/cancel)
FRONTEND_BASE_URL=http://localhost:4200

# Stripe keys
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=
